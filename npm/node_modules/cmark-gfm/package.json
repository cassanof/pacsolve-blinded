{
  "name": "cmark-gfm",
  "version": "0.8.5",
  "description": "Node bindings to GitHub's GFM-enhanced fork of the cmark Markdown parser",
  "repository": "https://github.com/BinaryMuse/node-cmark-gfm",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp build",
    "build:dev": "node-gyp build --debug",
    "rebuild": "node-gyp rebuild",
    "rebuild:dev": "node-gyp rebuild --debug",
    "prebuild:all": "yarn prebuild:napi && yarn prebuild:node",
    "prebuild:node": "prebuild -t 10.0.0 -t 12.0.0 -t 14.0.0 -t 15.0.0 -r node",
    "prebuild:napi": "prebuild -t 3 -r napi",
    "test": "mocha lib/test/helper.js lib/test/*.test.js && npm run tsd",
    "tsd": "tsd",
    "update-vendor": "./script/update-vendor",
    "upload": "node ./script/upload.js"
  },
  "keywords": [
    "markdown",
    "cmark",
    "commonmark",
    "gfm",
    "cmark-gfm"
  ],
  "author": "Michelle Tilley <michelle@michelletilley.net>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "tsd": {
    "directory": "lib/test"
  },
  "binary": {
    "napi_versions": [
      2,
      3
    ]
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^3.0.0",
    "prebuild-install": "^6.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-html": "^1.3.0",
    "dedent": "^0.7.0",
    "mocha": "^8.0.1",
    "node-gyp": "^7.0.0",
    "prebuild": "^10.0.0",
    "tsd": "^0.13.1",
    "typescript": "^4.0.2"
  }
}
